@inproceedings{naseh_optimization_2022,
	address = {Zahedan, Iran, Islamic Republic of},
	title = {Optimization of {Recloser}-{Fuse} {Coordination} by {Considering} {Distributed} {Generation} and {FCL}},
	copyright = {https://doi.org/10.15223/policy-029},
	isbn = {978-1-6654-8437-4},
	url = {https://ieeexplore.ieee.org/document/9763264/},
	doi = {10.1109/IPAPS55380.2022.9763264},
	abstract = {The most common protection equipment for distribution networks is a recloser and a fuse. The existence of distributed generation sources affects the short circuit level and disrupts the distribution network's recloser-fuse coordination. Using a fault current limiter in series with distributed generating sources is one solution to this problem. The fault current limiter is employed in this paper to restore recloser-fuse coordination. The system's presence or absence of distributed generation sources is unpredictable, and these unknown factors change the equipment's operation time and disrupt the recloser-fuse synchronization. Constraints connected to diverse types of connection of distributed generating sources in the coordination problem have been considered in this paper to address this issue. The protective equipment settings and the fault current limiter are calculated simultaneously in the proposed technique, coordination between the recloser-fuse would be complete and reduced recloser and fuse operating time and fault current limiter cost. The problem has been solved through the use of an analytical optimization method. The suggested method is used to a sample network to determine the best fault current limiter and protection settings while taking uncertainty into account.},
	language = {en},
	urldate = {2025-04-02},
	booktitle = {2022 {International} {Conference} on {Protection} and {Automation} of {Power} {Systems} ({IPAPS})},
	publisher = {IEEE},
	author = {Naseh, Mojtaba},
	month = jan,
	year = {2022},
	pages = {1--6},
	annote = {O equipamento de proteção mais comum para redes de distribuição é um religador e um fusível. A existência de fontes de geração distribuída afeta o nível de curto-circuito e interrompe a coordenação religador-fusível da rede de distribuição. A utilização de um limitador de corrente de falta em série com fontes de geração distribuída é uma das soluções para este problema. Os ajustes do equipamento de proteção e o limitador de corrente de falta são calculados simultaneamente na técnica proposta, a coordenação entre o religador e o fusível seria completa e o tempo de operação do religador e do fusível e o custo do limitador de corrente de falta seriam reduzidos.
},
	file = {PDF:/home/leonardofdsantos/Zotero/storage/ALGCK8B3/Naseh - 2022 - Optimization of Recloser-Fuse Coordination by Considering Distributed Generation and FCL.pdf:application/pdf},
}

