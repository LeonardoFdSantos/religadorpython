@inproceedings{fazaeli_impact_2022,
	address = {Zahedan, Iran, Islamic Republic of},
	title = {The {Impact} of a {Distributed} {Generation} and {Electric}-{Vehicle} {Parking} on the {Coordination} of the {Fuse}-{Recloser}},
	copyright = {https://doi.org/10.15223/policy-029},
	isbn = {978-1-6654-8437-4},
