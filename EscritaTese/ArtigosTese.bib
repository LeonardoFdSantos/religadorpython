@article{spagnolo_martins_evaluation_2023,
	title = {Evaluation on safety of people on ground generated voltages in unconventional networks},
	volume = {2023},
	issn = {2732-4494},
	url = {http://digital-library.theiet.org/doi/10.1049/icp.2023.0506},
	doi = {10.1049/icp.2023.0506},
	abstract = {In an electrical power distribution system, electrical grounding is essential, especially in networks with ground return. However, in rural areas, the cost of a three-phase network can be prohibitive, which limits consumers to single-phase networks like MRT or MRN. Recent research has proposed a new approach using earth as the phase conductor in a two-wire three-phase system (T2F), which makes the installation of three-phase networks in rural areas financially viable. This innovative system not only occupies a large part of the existing structure, but also provides three-phase power. Despite the cost savings, security issues need to be addressed before implementing this type of topology. The aim of this study is to evaluate the safety features of the T2F system to ensure its safe operation. The evaluations and safety equations were made in a virtual environment, but the soil stratification was based on results obtained in a test terrain that proved to be safe, even the grounding systems using existing rods and several rods.},
	language = {en},
	number = {6},
	urldate = {2025-04-07},
	journal = {IET Conference Proceedings},
	author = {Spagnolo Martins, M. and Stefanello, R. and Borré Sebastiany, P. H. and Da Silva Dos Santos, L. F. and Kraulich, L. and De Freitas Silveira, L. and Berlezi Ramos, D. and Cardoso Junior, G. and Matias Rêgo, R.},
	month = jul,
	year = {2023},
	pages = {3949--3953},
	file = {PDF:/home/leonardofdsantos/Zotero/storage/MWYYCFY8/Spagnolo Martins et al. - 2023 - Evaluation on safety of people on ground generated voltages in unconventional networks.pdf:application/pdf},
}

@inproceedings{leonardo_de_freitas_silveira_sistema_2022,
	address = {Santa Maria, Rio Grande do Sul, Brasil},
	title = {{SISTEMA} {DE} {DISTRIBUIÇÃO} {T2F}: {VERIFICAÇÃO} {DA} {EFETIVIDADE} {DAS} {METODOLOGIAS} {DE} {COMPENSAÇÃO} {DE} {DESEQUILÍBRIOS} {DE} {TENSÃO}},
	shorttitle = {{SISTEMA} {DE} {DISTRIBUIÇÃO} {T2F}},
	url = {https://www.sba.org.br/open_journal_systems/index.php/sbse/article/view/3080},
	doi = {10.20906/sbse.v2i1.3080},
	abstract = {This work presents a study of the effectiveness of the compensation impedance calculation methodology for the intrinsically unbalanced three-phase power distribution system (T2F). The calculation methodology in question is based on the Carson’s image method to obtain impedance values that, when arranged longitudinally and transversally to the system, equalize it in order to reduce voltage unbalance levels (\%DT). The comparison of the effectiveness of the calculated compensation is performed through the proposal of a computational method to obtain the impedance values that equalize the system in order to completely eliminate the unbalance between the phases. This study shows that factors such as the value of impedances implemented and their arrangement in the system directly imply the results of \%DT measured in the load coupled to the circuit.},
	language = {pt},
