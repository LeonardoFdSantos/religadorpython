	file = {PDF:/home/leonardofdsantos/Zotero/storage/L5LZBQGM/Katsivelakis et al. - 2020 - Transient Stability Analysis in Power Systems Integrated with a Doubly-Fed Induction Generator Wind.pdf:application/pdf},
