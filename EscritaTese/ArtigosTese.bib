@inproceedings{pachanapan_centralised_2023,
	address = {Chiang Mai, Thailand},
	title = {A {Centralised} {Fault} {Location}, {Isolation}, and {Service} {Restoration} {Approach} for {Rural} {Microgrid} {System} using {Reclosers} and {Sectionalizers}},
	copyright = {https://doi.org/10.15223/policy-029},
	isbn = {979-8-3503-1809-8},
	url = {https://ieeexplore.ieee.org/document/10561969/},
	doi = {10.1109/APPEEC57400.2023.10561969},
	abstract = {The automated fault location, isolation, and service restoration (FLISR) solution for the radial distribution network with distribution generation (DG), based on a centralized control structure, is proposed in this paper. This FLISR technique allows for enhanced coordination of reclosers and sectionalizers already present for protection or sectionalization purposes on the distribution network. The number of trip counters of reclosers and sectionalizers during the fault is used to identify the fault location. After locating a fault segment, the FLISR central controller will send the command to specific sectionalizers to isolate the fault from the other parts. The DG can be re-connected to operate in either grid-connected or island mode to support the service restoration process. The decision to reconnect the DG depends on the switch statuses after the fault is isolated. The purposed FLISR method is applied in the rural micro grid system consisting of a radial distribution network with a small-scale synchronous generator. The test system includes two reclosers and four sectionalizers which all of them can communicate with the FLISR central controller. The performances of automated FLISR operation in various fault locations are investigated using RMSsimulation on DIgSILENT PowerFactory software. The results show that using the information from trip counters and switch status, the rural microgrid system can effectively perform the FLISR process, improving network reliability.},
	language = {en},
	urldate = {2025-04-02},
	booktitle = {2023 {IEEE} {PES} 15th {Asia}-{Pacific} {Power} and {Energy} {Engineering} {Conference} ({APPEEC})},
	publisher = {IEEE},
	author = {Pachanapan, Piyadanai and Supannon, Autthaporn},
	month = dec,
	year = {2023},
	pages = {1--6},
	annote = {Este artigo apresenta uma solução automatizada para localização, isolamento e restauração de serviços (FLISR) em redes de distribuição radiais com geração distribuída (GD). A técnica FLISR proposta utiliza contadores de disparo de religadores e seccionalizadores para identificar a localização de faltas. Um controlador centralizado envia comandos para isolar o segmento defeituoso e reconectar a GD para restaurar o serviço. O método FLISR foi testado em um sistema de microgrid rural, demonstrando melhorias na confiabilidade da rede.
},
	file = {PDF:/home/leonardofdsantos/Zotero/storage/85LF5E4H/Pachanapan e Supannon - 2023 - A Centralised Fault Location, Isolation, and Service Restoration Approach for Rural Microgrid System.pdf:application/pdf},
}

