@inproceedings{katsivelakis_transient_2020,
	location = {Piraeus, Greece},
	title = {Transient Stability Analysis in Power Systems Integrated with a Doubly-Fed Induction Generator Wind Farm},
	rights = {https://ieeexplore.ieee.org/Xplorehelp/downloads/license-information/{IEEE}.html},
	isbn = {978-1-6654-2228-4},
