	author = {Lu, Zhengmei and Yan, Wei and Huang, Dezhi and Tang, Junjie and Ponci, Ferdinanda and Monti, Antonello},
	year = {2022},
	pages = {1--12},
	annote = {Neste artigo, um conjunto de equações de medição novas e aprimoradas são formuladas para barramentos medidos pelo método de dois wattímetros (TWM) e as medições incluem potência complexa linha a linha e corrente virtual de sequência zero. Juntamente com o aumento do cabo subterrâneo, o ponto neutro do alimentador MV é normalmente aterrado através de uma pequena resistência, e sua magnitude de tensão não é igual a zero. Consequentemente, um modelo preciso para o barramento slack, juntamente com suas equações de medição, é proposto aqui, com base nas medições convencionais e na medição virtual do ponto neutro. Além disso, este trabalho apresenta um modelo de SE trifásico para sistemas de distribuição MV e LV que incorpora as restrições de corrente de sequência zero e injeção zero, em combinação com o método de mínimos quadrados exponencialmente ponderados (EWLS).
},
	file = {PDF:/home/leonardofdsantos/Zotero/storage/53I4DJ5X/Lu et al. - 2022 - State Estimation for Medium- and Low-Voltage Distribution Systems Considering Measurement Characteri.pdf:application/pdf},
}

@inproceedings{mia_system_2023,
