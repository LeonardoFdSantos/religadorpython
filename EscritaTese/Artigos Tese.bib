	urldate = {2025-04-02},
	date = {2022-06-15},
	langid = {english},
	file = {PDF:/home/leonardofdsantos/Zotero/storage/67YWYTVN/Bueno De Souza et al. - 2022 - Accelerated Protection (AP) scheme in overhead distribution networks.pdf:application/pdf},
